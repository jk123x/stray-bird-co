{
  "name": "astro-shopify-sanity",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "keywords": [
    "Astro",
    "Shopify",
    "Sanity",
    "Headless CMS",
    "Ecommerce",
    "Visual Editing"
  ],
  "scripts": {
    "check": "astro check",
    "astro": "astro",
    "astro:dev": "astro dev",
    "dev": "run-p sanity:typegen astro:dev",
    "astro:build": "astro build",
    "build": "run-s sanity:typegen astro:build",
    "start": "astro dev",
    "preview": "astro preview",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "sanity:typegen": "sanity schema extract --enforce-required-fields --path ./src/sanity/schema.json && sanity typegen generate",
    "validate": "sanity-template-validate"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/netlify": "^6.3.4",
    "@astrojs/react": "^4.3.0",
    "@astrojs/svelte": "^7.1.0",
    "@nanostores/persistent": "^1.0.0",
    "@sanity/astro": "^3.2.6",
    "@sanity/client": "^7.4.0",
    "@sanity/color-input": "^4.0.3",
    "@sanity/icons": "^3.7.0",
    "@sanity/image-url": "^1.1.0",
    "@sanity/ui": "^2.15.18",
    "@sanity/vision": "^4.22.0",
    "@shopify/hydrogen-react": "^2025.5.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/vite": "^4.1.8",
    "astro": "^5.8.1",
    "astro-portabletext": "^0.11.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "groq": "^3.90.0",
    "nanostores": "^1.0.1",
    "pluralize-esm": "^9.0.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-is": "^19.1.0",
    "sanity": "^4.22.0",
    "slug": "^10.0.0",
    "styled-components": "^6.1.18",
    "svelte": "^5.33.13",
    "vite": "^6.3.5",
    "zod": "^3.25.47"
  },
  "devDependencies": {
    "@sanity/template-validator": "^2.4.3",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/slug": "^5.0.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3"
  }
}
